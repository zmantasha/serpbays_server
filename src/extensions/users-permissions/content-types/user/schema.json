{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 8,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "Advertiser": {
      "type": "boolean",
      "default": false
    },
    "Publisher": {
      "type": "boolean",
      "default": true
    },
    "withdrawalRequests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::withdrawal-request.withdrawal-request",
      "mappedBy": "publisher"
    },
    "user_wallet": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-wallet.user-wallet",
      "mappedBy": "users_permissions_user"
    },
    "advertiserOrders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "advertiser"
    },
    "publisherOrders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "publisher"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "owner"
    },
    "communications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::communication.communication",
      "mappedBy": "sender"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "businessName": {
      "type": "string"
    },
    "billingAddress": {
      "type": "string"
    },
    "registrationNumber":{
      "type":"string"
    },
    "city":{
      "type":"string"
    },
    "country": {
      "type": "string"
    },
    "pincode": {
      "type": "string"
    },
    "vatGstNumber": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "identity": {
      "type": "enumeration",
      "enum": ["SEO", "Agency", "Other"]
    },
    "invoices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice.invoice",
      "mappedBy": "user"
    },
    "transactions": {
          "type": "relation",
          "relation": "oneToMany",
          "target": "api::transaction.transaction",
          "mappedBy": "users_permissions_user"
        },
    "notificationPreferences": {
      "type": "json",
      "configurable": true,
      "private": false,
      "default": {
        "notifyMarketplaceNewItemEmail": true,
        "notifyMarketplaceNewItemApp": true,
        "notifyOrderStatusChangeEmail": true,
        "notifyOrderStatusChangeApp": true,
        "notifyOrderMessagesEmail": true,
        "notifyOrderMessagesApp": true,
        "notifySecurityAlertsEmail": true,
        "notifySecurityAlertsApp": true,
        "notifyWalletBillingUpdatesEmail": true,
        "notifyWalletBillingUpdatesApp": true,
        "notifyPlatformNewsEmail": false,
        "notifyPlatformNewsApp": true,
        "notifyPromotionsEmail": false,
        "notifyPromotionsApp": false
      }
    },
    "saved_filters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::saved-filter.saved-filter",
      "mappedBy": "users_permissions_user"
    }
  }
}
