{
  "kind": "collectionType",
  "collectionName": "marketplaces",
  "info": {
    "singularName": "marketplace",
    "pluralName": "marketplaces",
    "displayName": "Marketplace",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "url": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "link_insertion_price": {
      "type": "integer",
      "min": 0
    },
    "tat": {
      "type": "integer"
    },
    "placement_speed": {
      "type": "enumeration",
      "enum": [
        "Ultra Fast",
        "Fast", 
        "Normal",
        "Slow"
      ],
      "default": "Normal"
    },
    "min_word_count": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "forbidden_gp_price": {
      "type": "integer",
      "min": 0
    },
    "forbidden_li_price": {
      "type": "integer",
      "min": 0
    },
    "sample_post": {
      "type": "text"
    },
    "backlink_type": {
      "type": "enumeration",
      "enum": [
        "Do follow",
        "No follow"
      ],
      "required": true,
      "default": "Do follow"
    },
    "category": {
      "type": "json",
      "required": true
    },
    "other_category": {
      "type": "json"
    },
    "guidelines": {
      "type": "text",
      "default": "N/A"
    },
    "backlink_validity": {
      "type": "string",
      "required": true
    },
    "ahrefs_dr": {
      "type": "integer",
      "min": 0,
      "max": 100
    },
    "ahrefs_traffic": {
      "type": "integer",
      "min": 0
    },
    "ahrefs_rank": {
      "type": "integer",
      "min": 0
    },
    "moz_da": {
      "type": "integer",
      "max": 100,
      "min": 0
    },
    "fast_placement_status": {
      "type": "boolean",
      "default": false
    },
    "publisher_name": {
      "type": "string",
      "required": true
    },
    "publisher_email": {
      "type": "email",
      "required": true
    },
    "publisher_price": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "publisher_forbidden_gp_price": {
      "type": "integer",
      "min": 0
    },
    "publisher_forbidden_li_price": {
      "type": "integer",
      "min": 0
    },
    "publisher_link_insertion_price": {
      "type": "integer",
      "min": 0
    },
    "semrush_authority_score": {
      "type": "integer",
      "min": 0,
      "max": 100
    },
    "semrush_traffic": {
      "type": "integer",
      "min": 0
    },
    "spam_score": {
      "type": "integer",
      "min": 0,
      "max": 100
    },
    "adv_crypto_pricing": {
      "type": "integer",
      "min": 0
    },
    "adv_casino_pricing": {
      "type": "integer",
      "min": 0
    },
    "adv_cbd_pricing": {
      "type": "integer",
      "min": 0
    },
    "publisher_crypto_pricing": {
      "type": "integer",
      "min": 0
    },
    "publisher_casino_pricing": {
      "type": "integer",
      "min": 0
    },
    "publisher_cbd_pricing": {
      "type": "integer",
      "min": 0
    },
    "similarweb_traffic": {
      "type": "integer",
      "min": 0
    },
    "ahrefs_referring_domain": {
      "type": "integer",
      "min": 0
    },
    "domain_zone": {
      "type": "string"
    },
    "only_with_us": {
      "type": "boolean",
      "default": false
    },
    "countries": {
      "type": "json"
    },
    "language": {
      "type": "json",
      "required": true
    },
    "blacklist_status": {
      "type": "enumeration",
      "enum": [
        "active",
        "inactive"
      ],
      "default": "active"
    },
    "sponsored": {
      "type": "boolean",
      "default": false
    },
    "ugc": {
      "type": "boolean",
      "default": false
    },
    "dofollow_link": {
      "type": "integer",
      "default": 1,
      "min": 0
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "website"
    },
    "shortlists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::shortlist.shortlist",
      "mappedBy": "marketplace"
    }
  }
}
